{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "bestof/merchant",
  "version": "0.1.0",
  "title": "Merchant",
  "category": "widgets",
  "icon": "location",
  "description": "Block to create a merchant (for a mapicle)",
  "attributes": {
    "name": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "address": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "isCuratorPick": {
      "type": "boolean",
      "source": "text",
      "selector": "div"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "merchant",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}